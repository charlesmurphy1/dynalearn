{"graph": 
    {
        "name": "ERGraph",
        "params":
        {
            "N": 100,
            "p": 0.1
        }
    },
 "dynamics": 
    {
        "name": "SISDynamics",
        "params":
        {
            "num_states": 2,
            "infection_prob": 0.04,
            "recovery_prob": 0.1,
            "init_param": "None"
        }
    },
 "model": 
    {
        "name": "GATMarkovBinaryPredictor",
        "params":
        {
            "n_hidden": [32],
            "n_heads": [6],
            "dropout": 0,
            "weight_decay": 0.0001,
            "bn_epsilon": 0.001,
            "bn_momentum": 0.99
        }
    },
 "data_generator": 
    {
        "name": "MarkovBinaryDynamicsGenerator",
        "params":
        {

            "T": 200,
            "batch_size": 100,
            "max_null_iter": 100,
            "gamma": 0,
            "num_sample": 10000,
            "num_graphs": 1
        }
    },
 "training": 
    {
        "optimizer": "Adam",
        "loss": "categorical_crossentropy",
        "learning_rate": 0.0001,
        "target_noise": 0.1,
        "steps_per_epoch": 5000,
        "epochs": 50
    },
 "np_seed": 1,
 "tf_seed": 2,
 "experiment_name": "cedar_experiment",
 "path": "../data/"
}
