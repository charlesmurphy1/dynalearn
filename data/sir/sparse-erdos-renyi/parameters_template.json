{"graph": 
    {
        "name": "ERGraph",
        "params":
        {
            "N": NUM_NODES,
            "p": DENSITY
        }
    },
 "dynamics": 
    {
        "name": "SIRDynamics",
        "params":
        {
            "num_states": 3,
            "infection_prob": 0.04,
            "recovery_prob": 0.08,
            "init_param": "None",
            "relevant_transitions": ["SI", "IR"]
        }
    },
 "model": 
    {
        "name": "GATMarkovBinaryPredictor",
        "params":
        {
            "n_hidden": [32],
            "n_heads": [1],
            "dropout": 0,
            "weight_decay": 0.0001,
            "bn_epsilon": 0.001,
            "bn_momentum": 0.99
        }
    },
 "data_generator": 
    {
        "name": "MarkovBinaryDynamicsGenerator",
        "params":
        {

            "T": 20,
            "batch_size": NUM_NODES,
            "max_null_iter": 10,
            "gamma": 0,
            "num_sample": NUM_SAMPLE,
            "num_graphs": 1
        }
    },
 "training": 
    {
        "optimizer": "Adam",
        "loss": "noisy_crossentropy",
        "learning_rate": 0.001,
        "schedule": {"epoch":10, "factor":2},
        "target_noise": 0,
        "steps_per_epoch": NUM_SAMPLE,
        "epochs": 20,
        "with_truth": WITH_TRUTH
    },
 "np_seed": 1,
 "tf_seed": 2,
 "path": "PATH_TO_EXP",
 "name": "EXP_NAME"
}